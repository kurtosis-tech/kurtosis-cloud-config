{
  "agent": {
    "run_as_user": "cwagent"
  },
  "logs": {
    "logs_collected": {
      "files": {
        "collect_list": [
          {
            "file_path": "/var/log/kurtosis_installation.log",
            "log_group_class": "STANDARD",
            "log_group_name": "/ec2/cloud/kurtosis_installation.log",
            "log_stream_name": "{KURTOSIS_NAME} - {instance_id}",
            "retention_in_days": 14
          }
        ]
      }
    }
  }
}
